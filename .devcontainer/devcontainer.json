{
    "name": "Dev Spark Jupyter",
    "dockerComposeFile": [
        "../docker-compose.yaml",
        "./docker-compose.override.yaml"
    ],
    "service": "jupyter-notebook",
    "workspaceFolder": "/home/spark/notebooks",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            },
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter"
            ]
        }
    },
    "remoteUser": "spark"
}
